{
  "expo": {
    "entryPoint": "./src/App.jsx",
    "name": "JoinMe",
    "slug": "join-me",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 30000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.k3rnl.joinme"
    },
    "android": {
      "package": "com.k3rnl.joinme",
      "googleServicesFile": "./google-services.json",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyC-8auXAHTy56ZoleQzQpXVGgrKU-rSGMQ"
        }
      },
      "adaptiveIcon": {
        "backgroundColor": "#fff",
        "foregroundImage": "./src/assets/launcher.png"
      }
    }
  }
}
